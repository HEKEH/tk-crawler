{
  "name": "tk-crawler-main-client",
  "version": "0.0.4",
  "description": "TK Crawler Main Client",
  "author": "Will Ho",
  "main": "dist-electron/main.js",
  "files": [
    "dist",
    "dist-electron",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "check-production-env": "node -e \"if(!require('fs').existsSync('../../.env.production')){console.error('.env.production file not found');process.exit(1)}\"",
    "build": "npm run check-production-env && vue-tsc && vite build && electron-builder --config electron-builder.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "tk-crack": "workspace:*"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/vue-query": "^5.66.11",
    "@types/global-agent": "^2.1.3",
    "@types/lodash": "^4.17.13",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "axios": "^1.7.9",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "element-plus": "^2.9.6",
    "get-system-proxy": "^1.0.0",
    "global-agent": "^3.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "rxjs": "^7.8.1",
    "tailwindcss": "^4.1.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-html": "^3.2.2",
    "vue": "^3.5.12",
    "vue-tsc": "^2.0.26"
  }
}